{
	"name": "qa-poc",
	"description": "Nextjs project for QA POC",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npx is-ci || npx only-allow pnpm",
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"setdev": "run-p husky:install git:config",
		"onboarding": "node ./scripts/onboarding.js",
		"prepare": "is-ci || run-s setdev onboarding",
		"dev": "next dev -p 8080",
		"build": "next build",
		"start": "next start -p 80",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
		"type-check": "tsc -p tsconfig.json",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"inflint": "inflint -c inflint.config.ts",
		"cy:start": "run-s build start",
		"depcheck": "depcheck --config .depcheckrc.json",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout"
	},
	"dependencies": {
		"axios": "0.27.2",
		"i18next": "21.6.16",
		"next": "12.1.6",
		"puppeteer": "13.7.0",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-i18next": "11.16.8",
		"react-responsive-carousel": "3.2.23"
	},
	"devDependencies": {
		"@commitlint/cli": "16.2.4",
		"@types/node": "17.0.31",
		"@types/puppeteer": "5.4.6",
		"@types/react": "18.0.8",
		"@types/react-dom": "18.0.3",
		"@typescript-eslint/eslint-plugin": "5.22.0",
		"@typescript-eslint/parser": "5.22.0",
		"chalk": "4.1.2",
		"commitizen": "4.2.4",
		"cz-vinyl": "1.3.2",
		"depcheck": "1.4.3",
		"eslint": "8.14.0",
		"eslint-config-next": "12.1.6",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"figlet": "1.5.2",
		"husky": "7.0.4",
		"hygen": "6.2.0",
		"inflint": "1.2.2",
		"is-ci": "3.0.1",
		"lint-staged": "12.4.1",
		"npm-run-all2": "5.0.0",
		"postcss": "8.3.3",
		"prettier": "2.6.2",
		"sass": "1.51.0",
		"stylelint": "14.8.2",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-recess-order": "3.0.0",
		"stylelint-config-standard-scss": "3.0.0",
		"typescript": "4.6.4"
	}
}
